    position:relative;
}
.video-box .video .video-img>img{
    width:370px;
    height:256px;
}
.video-box .video .video-img .video-label{
    position:absolute;
    top:20px;
    left:20px;
}
.video-box .video .video-name{
    padding:26px 20px 0;
}
