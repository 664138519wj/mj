}

/*mouse over*/
.video-box .video:hover{
    box-shadow: 0 0 3px 3px rgba(218,218,218,.4);
    -webkit-transform: scale(1.1);
    -moz-transform: scale(1.1);
    -ms-transform: scale(1.1);
    -o-transform: scale(1.1);
    transform: scale(1.1);
}
.video-box .video:hover .video-name h3,
.video-box .video:hover .video-name p span{
    color:#fd9938;
}
.video-box .video:hover .video-name p .video-pic1{
    display: none;
}
.video-box .video:hover .video-name p .video-pic2{
    display: block;
}
