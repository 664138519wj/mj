}
/* 侧边栏 */
.index-menu{
    width: 58px;
    border:1px solid #ccc;
    border-radius: 4px;
    position: fixed;
    top:50%;
    transform: translateY(-50%);
    -webkit-transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    -moz-transform: translateY(-50%);
    left: 15px;
    z-index: 99999;
}
.index-menu ul{
    padding:4px 0;
    background:#fff;
}
.index-menu ul li{
    text-align: center;
    margin:0 4px;
    border-bottom:1px solid #ccc;
}
.index-menu ul li.selected{
    background:#fd9938;
    border-radius:4px;
}
.index-menu ul li:last-child{
    border-bottom:0;
}
.index-menu ul li a,
.index-menu ul li.selected span{
    font-size: 14px;
    display: inline-block;
    padding: 10px 0;
}
.index-menu ul li.selected span{
    color:#fff;
}
.index-menu ul li:hover a{
    color:#fd9938;
}
.index-right-menu{
    position: fixed;
    right: 15px;
    bottom: 90px;
    width: 50px;
    border-radius:4px;
    border:1px solid #fd9938;
    background:#fff;
    text-align: center;
}
.index-right-menu a{
    font-size: 14px;
    color:#fd9938;
}



/*Video course*/
.index-video{
    margin-top:138px;
    background:#fff;
    padding:76px 0 66px;
}
.video-box{
