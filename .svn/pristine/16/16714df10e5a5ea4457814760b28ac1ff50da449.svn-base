/*platform_course*/
.index-course{
    padding-top: 80px;
}
.course-box{
    margin-top:40px;
    position: relative;
}
.course-box ul li{
    background:#fff;
    padding:32px;
    margin-bottom:28px;
}
.course-box ul li:after{
    content: '';
    display: block;
    clear: both;
}
.course-box ul li .course-left{
    width:700px;
}
.course-box ul li .course-left h2,
.course-box ul li .course-left>p{
    width:700px;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    margin-bottom:20px;
}
.course-box ul li .course-left h2{
    padding-bottom:20px;
    font-size: 20px;
}
.course-box ul li .course-left .figcaption{
    margin-bottom:20px;
    width:700px;
    height:48px;
    line-height:24px;
}
.course-box ul li .course-left .figcaption p{
	height:48px;
	overflow-y:hidden;
}
.course-box ul li .course-left .price{
    margin-top:16px;
}
.course-box ul li .course-left p,
.course-box ul li .course-left .price span{
    color:#666;
}
.course-box ul li .course-left .price b{
    font-size: 22px;
    color:#fd9938;
}
.course-box ul li .course-right,
.course-box ul li .course-right img{
    width:320px;
    height:224px;
}

/*mouse over*/
.course-box ul li:hover .course-left h2{
