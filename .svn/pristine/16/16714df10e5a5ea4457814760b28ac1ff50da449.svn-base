@charset="UTF-8";
/*index*/
/*banner*/
.swiper-slide{
	width:100%!important;
}
.swiper-container img{
    width:100%;
    height:220px;
}
.swiper-pagination-bullet{
    width:60px;
    height:4px;
    border-radius: 1.5px;
    background:rgba(255,255,255,.6);
}
.swiper-pagination-bullet-active{
    background:#fff;
}
/*content*/
.content{
    background: #f5f5f7;
}
.index-title{
    margin:0 auto;
    width:488px;
    height:54px;
    line-height:54px;
    text-align: center;
    background:#2f3232;
    color:#fff;
}
.index-label{
    background:#fff;
    padding:60px 0 80px;
}
.index-label .label-box{
    width:136px;
    float: left;
    text-align: center;
    margin-right: 114px;
}
.index-label .label-box a,
.plat-teacher .plat-box a,
.course-box ul li a,
.video-box .video a,
.label-add a,
.mock-box .test .test-btn a,
.traffic-main ul li a,
.more-article ul li a,
.info-box li a,
.notice-box li a
{
    display: inline-block;
    width:100%;
    height:100%;
}
.index-label .label-box .index-img-box{
    width:136px;
    height:136px;
    line-height:136px;
    text-align: center;
    border-radius: 50%;
    border:2px solid #2f3232;
}
.index-label .label-box:first-child{
    margin-left:22px;
}
.index-label .label-box:last-child{
    margin-right:22px;
}
.index-label .label-box .label-img2{
    display: none;
}
.index-label .label-box p{
    padding-top:30px;
    font-weight: 600;
    font-size: 24px;
}
/*mouse over*/
.index-label .label-box:hover .index-img-box{
    border:2px solid #fd9938;
    background:#fd9938;
    position: relative;
}
.index-label .label-box:hover .label-img1{
    display: none;
}
.index-label .label-box:hover .label-img2{
    display: block;
    position:absolute;
    top:50%;
    left:50%;
    -webkit-transform: translate(-50%,-50%);
    -moz-transform: translate(-50%,-50%);
    -ms-transform: translate(-50%,-50%);
    -o-transform: translate(-50%,-50%);
    transform: translate(-50%,-50%);
}

/*The training teacher*/
.index-platform{
    padding-top:70px;
}
.plat-title{
    text-align: center;
}
.plat-title h2{
    font-size:20px;
}
.plat-title b{
    font-size:16px;
}
.plat-title p{
