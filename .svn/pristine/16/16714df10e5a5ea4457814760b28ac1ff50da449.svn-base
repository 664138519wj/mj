    background:#f4f4f4;
    font-size: 18px;
    padding-left:30px;
    margin-top:15px;
}
.login .login-info .input-box .hide-img,
.login .login-info .input-box .show-img{
    width: 25px;
    height:20px;
    position: absolute;
    right:20px;
    bottom:18px;
}
.login .login-info .input-box .show-img{
    display: none;
}
.login .login-info .forget{
    margin-top:18px;
}
.login .login-info .forget a{
