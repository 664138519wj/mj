package com.migu.online.param;

import java.math.BigDecimal;
import java.util.ArrayList;
import java.util.List;

import com.migu.online.model.VipCourse;

/**
 * 驾校报名param
 * @author fanyunlong
 *
 */
public class VipCourseParam {

	private List<VipCourse> cList = new ArrayList<VipCourse>();
	
	private BigDecimal tPrice = new BigDecimal(0);
		
	private int number = 0;
	
	private String kemus = "";
	
	private String beginTimeStr = "";

	public List<VipCourse> getcList() {
		return cList;
	}

	public void setcList(List<VipCourse> cList) {
		this.cList = cList;
	}

	public BigDecimal gettPrice() {
		return tPrice;
	}

	public void settPrice(BigDecimal tPrice) {
		this.tPrice = tPrice;
	}

	public int getNumber() {
		return number;
	}

	public void setNumber(int number) {
		this.number = number;
	}

	public String getKemus() {
		return kemus;
	}

	public void setKemus(String kemus) {
		this.kemus = kemus;
	}

	public String getBeginTimeStr() {
		return beginTimeStr;
	}

	public void setBeginTimeStr(String beginTimeStr) {
		this.beginTimeStr = beginTimeStr;
	}

}
